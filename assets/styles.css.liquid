body {
  font-family: {{ base_font.family }}, {{ base_font.fallback_families }};
  font-weight: {{ base_font.weight }};
  font-style: {{ base_font.style }};
  font-size: {{ font_size_base_int }}px;
  line-height: 1.6em;
  color: {{ font_col_base }};
  background-color: {{ bg_col }};
  /* Fonts on Macs look bad without these */
  -moz-font-feature-settings: "liga";
  -moz-osx-font-smoothing: grayscale;
  -webkit-font-feature-settings: "liga";
  -webkit-font-smoothing: antialiased;
}

{% if settings.bg_image != blank %}
body::before {
  content: "";
  position: fixed;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
  z-index: -1;
  transform: translate3d(0, 0, 0);
    background-image: url({{ settings.bg_image | img_url: '2048x' }});
  {% if settings.bg_method contains 'full' %}
  background-repeat: no-repeat;
  background-size: cover;
  background-position: center center;
  {% elsif settings.bg_method contains 'repeatxy' %}
  background-repeat: repeat;
  {% elsif settings.bg_method contains 'repeatx' %}
  background-repeat: repeat-x;
  {% elsif settings.bg_method contains 'norepeat' %}
  background-repeat: no-repeat;
  {% endif %}
  {% if settings.bg_method contains 'centertop' %}
  background-position: center top;
  {% endif %}
}

{% endif %}
h1, .h1, .h1-style {
  font-size: 125%;
  line-height: 1.3;
  margin: 1em 0;
}

